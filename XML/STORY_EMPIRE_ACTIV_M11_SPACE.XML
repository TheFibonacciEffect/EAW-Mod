		<Event_Param1>3</Event_Param1>
		<Reward_Type>COMMANDBAR_MOVIE</Reward_Type>
		<Reward_Param1>Commander_Moff_Loop</Reward_Param1>
		<Prereq>Empire_A04M11_Begin</Prereq>
	</Event>

			<Event Name="Empire_ActIV_M11_Intro_Dialog_00a">
