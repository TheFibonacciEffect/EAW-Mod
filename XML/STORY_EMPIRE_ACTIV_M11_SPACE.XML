			<Reward_Param1>Empire</Reward_Param1>
			<Prereq>Empire_A04M11_Alderaan_Destroyed_Notification</Prereq>
		</Event>
	
		<Event Name="Empire_A04M011_Increment_Win">
			<Event_Type>STORY_VICTORY</Event_Type>
			<Event_Param1>EMPIRE</Event_Param1>
