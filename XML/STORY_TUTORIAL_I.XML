	</Event>
   

	<!-- Fly a fleet over to Kashyyyk for extra dramatic effect! -->
   <Event Name="Tutorial_I_Move_Kashyyk_Attack_Fleet">
      <Event_Type>STORY_ELAPSED</Event_Type>
      <Event_Param1>1</Event_Param1>
      <Reward_Type>MOVE_FLEET</Reward_Type>
      <Reward_Param1>NalHutta</Reward_Param1>
      <Reward_Param2>Kashyyyk</Reward_Param2>
      <Reward_Param3>FILTER_ENEMY_ONLY</Reward_Param3>
      <Prereq>Tutorial_I_Intro_02</Prereq>
   </Event>

	<!-- Change faction of Kashyyyk -->
	<Event Name="Tutorial_I_Reset_Kashyyyk_Faction">
		<Event_Type>STORY_ENTER</Event_Type>
		<Event_Param1>Kashyyyk</Event_Param1>
		<Reward_Type>PLANET_FACTION</Reward_Type>
		<Reward_Param1>Kashyyyk</Reward_Param1>
		<Reward_Param2>Neutral</Reward_Param2>
		<Prereq>Tutorial_I_Move_Kashyyk_Attack_Fleet</Prereq>
	</Event>
	
	
	<Event Name="Tutorial_I_Link_Tactical">
		<Event_Type>STORY_TRIGGER</Event_Type>
		<!--<Event_Type>STORY_ENTER</Event_Type>
		<Event_Param1>Kashyyyk</Event_Param1>
		<Event_Param2>FILTER_ENEMY_ONLY</Event_Param2>-->
		<Reward_Type>LINK_TACTICAL</Reward_Type>
		<Reward_Param1>Kashyyyk</Reward_Param1>
		<Reward_Param2>LAND</Reward_Param2>
		<Reward_Param3>Rebel</Reward_Param3>
		<Reward_Param4>TM01_Gaining_Ground.ted</Reward_Param4>
		<Reward_Param5>Empire</Reward_Param5>
		<Reward_Param7>Story_Plots_Tutorial_M01_LAND.XML</Reward_Param7>
		<Reward_Param8>1</Reward_Param8>
		<!-- Do we want the prebattle cinematic to play?-->
		<Reward_Param9>0</Reward_Param9>
		<!-- Do we want to start the scene faded out?-->
		<Reward_Param10>1</Reward_Param10>
		<!-- Do we want to start the scene in letter box mode-->
		<Reward_Param11>1</Reward_Param11>
		<!-- Show the Battle Pending dialog? -->
		<Reward_Param13>0</Reward_Param13>
		<Prereq>Tutorial_I_Reset_Kashyyyk_Faction</Prereq>
      <Branch>BRANCH_TUTORIAL_M01</Branch>
	</Event>
	
	
	<!-- Click the Begin button -->	
	<Event Name="Tutorial_I_Auto_Click_Begin">
		<Event_Type>STORY_TRIGGER</Event_Type>
      <Reward_Type>FORCE_CLICK_GUI</Reward_Type>
      <Reward_Param1>choice_button_left</Reward_Param1>
		<Prereq>Tutorial_I_Link_Tactical</Prereq>
	</Event>
	
	
	
   <!-- Wait until the battle summary dialog is closed -->
	<Event Name="Tutorial_I_Returned_01">
		<Event_Type>STORY_GENERIC</Event_Type>
      <Event_Param1>battle_end_closed</Event_Param1>
		<Prereq>Tutorial_I_Link_Tactical</Prereq>
      <Branch>BRANCH_TUTORIAL_M01</Branch>
	</Event>


<!-- Tutorial 1 failed -->

   <!-- Detect mission 1 being lost.  Display text telling the player to try again -->
	<Event Name="Tutorial_I_Failed">
		<Event_Type>STORY_MISSION_LOST</Event_Type>
		<Event_Param1>Story_Plots_Tutorial_M01_LAND.XML</Event_Param1>
      <Prereq>Tutorial_I_Returned_01</Prereq>
      <Branch>BRANCH_TUTORIAL_M01</Branch>
	</Event>

   <!-- Re-enable mission -->
	<Event Name="Tutorial_I_Failed_02">
		<Event_Type>STORY_TRIGGER</Event_Type>
      <Reward_Type>RESET_BRANCH</Reward_Type>
      <Reward_Param1>BRANCH_TUTORIAL_M01</Reward_Param1>
		<Prereq>Tutorial_I_Failed</Prereq>
      <Branch>BRANCH_TUTORIAL_M01</Branch>
	</Event>



<!-- Mission 4 ends successfully -->

	<Event Name="Tutorial_I_Completed">
